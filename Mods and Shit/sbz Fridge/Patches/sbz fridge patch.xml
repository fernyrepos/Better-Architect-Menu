<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
<!-- FOOD STORAGE -->
	
	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[@Name="sbzFridgeBase"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_FoodStorage</designationCategory>
		</value>
	</Operation>
	
<!-- GENERAL STORAGE -->

	<Operation Class="PatchOperationReplace">
		<success>Always</success>
		<xpath>Defs/ThingDef[defName="sbz_CorpseFridge"]/designationCategory</xpath>
		<value>
			<designationCategory>Ferny_GeneralStorage</designationCategory>
		</value>
	</Operation>
	
</Patch>
